{
  "name": "enterprise-code-ai-tooling",
  "version": "0.1.0",
  "description": "MCP server for enterprise code AI tooling",
  "type": "module",
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "start": "node build/index.js",
    "test": "npm run test:mcp",
    "test:mcp": "npm run build && node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand --testMatch \"**/tests/**/*.test.mjs\""
  },
  "dependencies": {
    "@langchain/community": "^0.2.24",
    "@langchain/openai": "^1.2.7",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@qdrant/js-client-rest": "^1.15.0",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "typescript": "^5.0.0"
  }
}
